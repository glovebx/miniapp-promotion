.flex-row {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    flex-direction: row;
}

.flex-col {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    flex-direction: column;
}

.flex-grow-0 {
    min-width: 0;
    -webkit-box-flex: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
    -ms-flex-negative: 0;
    flex-shrink: 0;
}

.flex-grow-1 {
    min-width: 0;
    -webkit-box-flex: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    -ms-flex-negative: 1;
    flex-shrink: 1;
}

.flex-x-center {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
}

.flex-y-center {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -ms-grid-row-align: center;
    align-items: center;
}

.MoneyInfo {
    margin-top: 420rpx;
}

.MenuList {
    margin-top: 10rpx;
}

.MenuList .MenuItem {
    height: 100rpx;
    /* padding-left: 35rpx; */
    /* padding-top: 30rpx; */
    font-size: 28rpx;
    display: flex;
    flex-direction: row;
    border-bottom: 1rpx solid #eeeeee;
    background: #fff;
    position: relative;
}

.MenuList .MenuItem.ContactBtn {
    height: 100rpx;
    line-height: 40rpx;
    border-radius: 0;
}

.MenuList .MenuItem.ContactBtn::after {
    border: 0;
}

.MenuItem .MenuUrl {
    width: 100%;
    padding-left: 35rpx;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}

.MenuItem .MenuIcon {
    width: 50rpx;
    height: 50rpx;
    float: left;
}

.MenuItem .MenuText {
    padding-left: 25rpx;
    float: left;
    padding-top: 4rpx;
}

@-webkit-keyframes move_wave {
    0% {
        transform: translateX(0) translateZ(0) scaleY(1);
    }

    50% {
        transform: translateX(-25%) translateZ(0) scaleY(0.55);
    }

    100% {
        transform: translateX(-50%) translateZ(0) scaleY(1);
    }
}

@keyframes move_wave {
    0% {
        transform: translateX(0) translateZ(0) scaleY(1);
    }

    50% {
        transform: translateX(-25%) translateZ(0) scaleY(0.55);
    }

    100% {
        transform: translateX(-50%) translateZ(0) scaleY(1);
    }
}

.waveWrapper {
    overflow: hidden;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    height: 420rpx;
    margin: auto;
}

.waveWrapperInner {
    position: absolute;
    width: 100%;
    overflow: hidden;
    height: 100%;
    bottom: -2rpx;
    background-image: linear-gradient(to top,#ff7979 20%,#ee4f4f 80%);
}

.bgTop {
    z-index: 10;
    opacity: 0.75;
}

.bgBottom {
    z-index: 5;
}

.wave {
    position: absolute;
    left: 0;
    width: 200%;
    height: 100%;
    background-repeat: repeat no-repeat;
    background-position: 0 bottom;
    transform-origin: center bottom;
}

.waveTop {
    background-image: url('data:image/png;base64,');
    background-size: 50% 240rpx;
}

.waveAnimation .waveTop {
    animation: move_wave 12s linear infinite;
}

.waveBottom {
    background-image: url('data:image/png;base64,');
    background-size: 50% 200rpx;
}

.waveAnimation .waveBottom {
    animation: move_wave 15s linear infinite;
}

.login-btn {
    width: 40%;
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    text-align: center;
    text-decoration: none;
    line-height: 2.55555556;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #fff!important;
    background-color: green!important;
}